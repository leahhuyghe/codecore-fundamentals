<h1>Listing all contacts</h1>

<table class="table table-striped">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone Number</th>
    <th>Email</th>
    <th>Note</th>
    <th></th>
    <th></th>
  </tr>
  <% @all_contacts.each do |contact| %>
    <tr>
      <td><%= contact.first_name %></td>
      <td><%= contact.last_name %></td>
      <td><%= contact.phone_number %></td>
      <td><%= contact.email %></td>
      <td><%= contact.note %></td>
      <td><a href="/note/<%= contact.id %>" class="btn btn-primary"
      >Add Note</a></td>
      <td><a href="/delete/<%= contact.id %>" class="btn btn-danger">Delete</a></td>
    </tr>
  <% end %>
</table>